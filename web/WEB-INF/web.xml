<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ComprobarUsuario</servlet-name>
        <servlet-class>com.fia.upeu.control.ComprobarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlUsuario</servlet-name>
        <servlet-class>com.fia.upeu.control.ControlUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlUsuarios</servlet-name>
        <servlet-class>com.fia.upeu.control.ControlUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControlUsuarios</servlet-name>
        <url-pattern>/ControlUsuarios</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
